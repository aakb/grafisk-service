/**
 * Enable field group and field ui.
 */
function grafisk_service_base_install() {
  // Enable modules.
  $module_list = [
    'field_ui',
    'field_group',
    'toolbar',
    'editor',
    'update',
    'inline_form_errors',
  ];
  \Drupal::service('module_installer')->install($module_list);
}
